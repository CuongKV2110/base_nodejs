<div class="mt-4">
    <div class="row">
        {{#each menus}}
        <div class="col-sm-6 col-lg-4">
            <div class="card">
                <a href="/item/{{this.type}}">
                <img src="{{this.image}}" class="card-img-top" alt="..."></a>
                
                <div class="card-body">
                    <h5 class="card-title">{{this.name}}</h5>
                
                    <a href="#" class="btn btn-primary">{{this.type}}</a>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
</div>